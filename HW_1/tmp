def test_process_file_queries_can_process_all_qutries_from_file(capsys):
	with open("query_utf8.txt", 'w') as test_file:
		test_file.write()
	with open("query_utf8.txt") as query_fin:
		process_file_queries(
			inverted_index_path=DEFAULT_INVERTED_INDEX_STORE_PATH,
			query_file=query_fin
		)
	captured = capsys.readouterr()
	assert "load inverted index" not in captured.out
	assert "load inverted index" in captured.err

def test_process_file_list_can_process_all_qutries_from_list(capsys):
	process_list_queries(
		inverted_index_path=DEFAULT_INVERTED_INDEX_STORE_PATH,
		query_list=[['two', 'tables'], ['one', 'map']]
	)
